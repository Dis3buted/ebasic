/*
Simple mandlbrot example, developed by Mike Bell
*/

outstr=""

x=-2.0
y=-1.0 + (coreid*0.125)

for i=0 to 66
  re = x
  im = y
  for j=0 to 20
    re = re*re - im*im + x
    im = 2*re*im + y
  next
  if re*re + im*im < 4 then
    outstr = outstr+"#"
  else
    outstr = outstr+" "
  end if
  x = x + 0.05
next

for i=0 to 15
  if coreid=i then
    print outstr
  end if
  sync
next
